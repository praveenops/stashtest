{
  "name": "ion-innovation-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --host=0.0.0.0 --disable-host-check --proxy-config api-proxy.json",
    "local": "ng serve --aot --host=0.0.0.0 --disable-host-check --proxy-config api-proxy.json -c local-dev ",
    "test": "ng test --code-coverage",
    "lint": "yarn tslint && yarn csslint",
    "build": "ng build",
    "build:dev": "ng build --prod -c=prod-dev --base-href=/ion/ui/",
    "build:si": "ng build --prod -c=si --base-href=/ion/ui/",
    "build:uat": "ng build --prod -c=uat --base-href=/ion/ui/",
    "build:prod": "ng build --prod -c=prod --base-href=/ion/ui/",
    "tslint": "ng lint ion-innovation-ui",
    "csslint": "stylelint '**/*.scss' --config stylelintrc.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@codebakery/origami": "^2.0.2",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "angulartics2": "^6.0.0",
    "bootstrap": "^4.0.0",
    "core-js": "^2.5.4",
    "hammerjs": "2.0.6",
    "ion-ui-components": "git+ssh://git@adlm.nielsen.com:7999/bion/ion-ui-components.git#v1.0.2",
    "lodash.isequal": "^4.5.0",
    "ngrx-store-freeze": "^0.2.2",
    "ngx-perfect-scrollbar": "^6.0.0",
    "rxjs": "^6.0.0",
    "svgjs": "^2.6.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
    "@angular/cli": "~6.0.1",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/d3-array": "^1.2.1",
    "@types/d3-axis": "^1.0.9",
    "@types/d3-collection": "^1.0.5",
    "@types/d3-scale": "^1.0.10",
    "@types/d3-selection": "^1.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "stylelint": "^9.2.0",
    "stylelint-scss": "^3.1.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
